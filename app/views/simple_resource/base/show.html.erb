<% @title ||= resource_title %>

<table class="<%= table_classes_for(:resource) %>">
  <tbody>
    <% resource_attributes.each do |attribute_name| %>
      <tr>
        <th><%= attribute_human_name(attribute_name) %></th>
        <td><%= attribute_value(resource, attribute_name) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
